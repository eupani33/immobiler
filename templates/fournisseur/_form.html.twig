{{form_start(form)}}
<div class="container mt-5">
	<div class="row">
		<div class="col-md-3">{{ form_row(form.nom)}}</div>
		<div class="col-md-2">{{ form_row(form.contact)}}</div>
	</div>

	<div class="row">
		<div class="col-md-5">{{ form_row(form.adresse)}}</div>
		<div class="col-md-5">{{ form_row(form.ville)}}</div>
		<div class="col-md-2">{{ form_row(form.cp)}}</div>
	</div>

	<div class="row">
		<div class="col-md-3">{{ form_row(form.email)}}</div>
		<div class="col-md-3">{{ form_row(form.tel)}}</div>

	</div>

	<div class="row justify-content-md-center">

		<div class="col col-lg-2">
			<button type="submit" class="btn btn-primary">Valider</button>
		</div>


		{{form_end(form)}}